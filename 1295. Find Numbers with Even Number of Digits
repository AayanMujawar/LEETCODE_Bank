int findNumbers(int* nums, int numsSize) {
    int countEven=0;
    for(int i = 0 ; i < numsSize ; i++)
    {
        int count = 0;
        int temp = nums[i];
        while (temp>0)
        {
            temp /= 10;
           
            count++;
        }
        if(count%2==0)
        {
            countEven++;
        }
    }
    return countEven;
}
