class Solution {
    public int minElement(int[] nums) {
        int min = Integer.MAX_VALUE;

        for (int num : nums) {
            int sum = 0;
            int x = num;
            while (x > 0) {
                sum += x % 10;
                x /= 10;
            }
            min = Math.min(min, sum);
        }

        return min;
    }
}
