struct ListNode* mergeInBetween(struct ListNode* list1, int a, int b, struct ListNode* list2) {
    struct ListNode *prevA = list1;
    struct ListNode *afterB = list1;

    for (int i = 0; i < a - 1; i++) {
        prevA = prevA->next;
    }

    for (int i = 0; i <= b; i++) {
        afterB = afterB->next;
    }

    prevA->next = list2;

    struct ListNode *tail = list2;
    while (tail->next != NULL) {
        tail = tail->next;
    }

    tail->next = afterB;

    return list1;
}
