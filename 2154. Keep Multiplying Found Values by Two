int findFinalValue(int* nums, int numsSize, int original) {
    int Original = original;
    int hash[10001] = {0};
    for(int i =0 ; i< numsSize ; i++)
    {
       hash[nums[i]]++;
    }
    for(int i = 0 ; i <numsSize ; i++)
    {
        if(hash[Original])
        {
            Original = Original * 2;
        }
    }
    return Original;
}
