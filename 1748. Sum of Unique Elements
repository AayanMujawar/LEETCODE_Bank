int sumOfUnique(int* nums, int numsSize) {
    int hash[101] = {0};
    for(int i =0 ; i < numsSize ; i++)
    {
        hash[nums[i]]++;
    }
    int count = 0;
     for(int i =0 ; i < numsSize ; i++)
    {
       if( hash[nums[i]] == 1)
       {
        count = count + nums[i];
       }
    }
    return count;
}
