int* findDuplicates(int* nums, int numsSize, int* returnSize) {
    int *result = (int*)malloc(numsSize * sizeof(int));
    int hashmap[100001] = {0};  
    int k = 0;

    for (int i = 0; i < numsSize; i++) {
        hashmap[nums[i]]++;
        if (hashmap[nums[i]] == 2) {
            result[k++] = nums[i];
        }
    }

    *returnSize = k;  
    return result;
}
