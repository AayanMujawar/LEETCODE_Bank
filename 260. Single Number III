
class Solution {
public:
    vector<int> singleNumber(vector<int>& nums) {
        int xor_all = 0;
        for (int num : nums)
            xor_all ^= num;

unsigned int diff_bit = xor_all & ((~(unsigned int)xor_all) + 1);


        int x = 0, y = 0;
        for (int num : nums) {
            if (num & diff_bit)
                x ^= num;
            else
                y ^= num;
        }

        return {x, y};
    }
};
