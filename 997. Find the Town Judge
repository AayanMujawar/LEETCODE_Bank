int findJudge(int n, int** trust, int trustSize, int* trustColSize) {
    int trusts[1001] = {0};
    int trustedBy[1001] = {0};
for (int i = 0; i < trustSize; i++) {
         trusts[trust[i][0]]++;
        trustedBy[ trust[i][1]]++;
    }
for (int i = 1; i <= n; i++) {
        if (trusts[i] == 0 && trustedBy[i] == n-1) {
            return i;
        }
    }
    return -1;
}
