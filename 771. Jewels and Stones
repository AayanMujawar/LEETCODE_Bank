int numJewelsInStones(char* jewels, char* stones) {
    char freq[200] = {0};
    int count = 0;

    for(int i =0 ; jewels[i]!='\0';i++)
    {
        freq[jewels[i]]++;
    }
    for(int i = 0 ; i < stones[i]!='\0';i++)
    {
        if(freq[stones[i]] > 0)
        {
            count++;
        }
    }
    return count;
}
