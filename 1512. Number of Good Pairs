int numIdenticalPairs(int* nums, int numsSize) {
    int hash[101]={0};
    int count = 0;
    for(int i =0 ; i < numsSize ; i++ )
    {
        hash[nums[i]]++;
    }

    for(int i = 0 ; i < 101 ; i++)
    {
        if(hash[i]  > 1)
        {
            int n = hash[i];
            count = count + (( n * (n - 1)) / 2 );
            
        }
    }
    return count;
}
