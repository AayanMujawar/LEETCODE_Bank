/**
 * Note: The returned array must be malloced, assume caller calls free().
 */
int* getSneakyNumbers(int* nums, int numsSize, int* returnSize) {
    int* result = (int*)malloc(2 * sizeof(int));
    int count = 0;
     int hash[103] = {0};

     for(int i =0 ; i< numsSize ; i++)
     {
        hash[nums[i]]++;
        if(hash[nums[i]] == 2)
        {
            result[count] = nums[i];
            count = count + 1;
           if(count == 2)
           {
            break;
           }
        }
        

     }
     *returnSize = count;
     return result;
}
