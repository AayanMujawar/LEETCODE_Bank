int findLucky(int* arr, int arrSize) {
    int hash[501] = {0};
    int lucky = -1;
 for(int i =0 ;i < arrSize ; i++)
    {
        hash[arr[i]]++;
    }
     for(int i =0 ;i < arrSize ; i++)
    {
        if(hash[arr[i]] == arr[i]  && (lucky < arr[i]))
        {
           
                lucky = arr[i];
            }
        }
    
    return lucky;
}
