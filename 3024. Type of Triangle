char* triangleType(int* nums, int numsSize) {
    int a = nums[0], b = nums[1], c = nums[2];
    
    // check triangle validity
    if ((a + b <= c) || (a + c <= b) || (b + c <= a)) {
        return "none";
    }

    if (a == b && b == c) {
        return "equilateral";
    } 
    else if (a == b || b == c || a == c) {
        return "isosceles";
    } 
    else {
        return "scalene";
    }
}
