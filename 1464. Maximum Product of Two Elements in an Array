#include<limits.h>
int maxProduct(int* nums, int numsSize) {
    if (numsSize < 2) return 0; // not enough elements for a product

    int max1 = INT_MIN, max2 = INT_MIN;

    for (int i = 0; i < numsSize; i++) {
        if (nums[i] >= max1) {
            max2 = max1;
            max1 = nums[i];
        } else if (nums[i] > max2) {
            max2 = nums[i];
        }
    }

    return (max1 - 1) * (max2 - 1);
}
